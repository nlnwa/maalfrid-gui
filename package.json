{
  "name": "maalfrid-gui",
  "version": "0.5.2",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c development",
    "start:dev-nb": "ng serve -c development-nb",
    "start:prod": "ng serve -c production",
    "start:prod-nb": "ng serve -c production-nb",
    "build": "node --max_old_space_size=4048 ./node_modules/.bin/ng build",
    "build:dev": "yarn build -c development",
    "build:dev-nb": "yarn build -c development-nb",
    "build:prod": "yarn build -c production",
    "build:prod-nb": "yarn build -c production-nb",
    "build:docker": "docker build -t norsknettarkiv/maalfrid-gui .",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "xi18n": "ng xi18n --output-path src/assets/locale --i18n-locale nn && xliffmerge  --profile xliffmerge.json nb nn"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/cdk": "~8.0.2",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "^8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.1.1",
    "@angular/material": "~8.0.2",
    "@angular/platform-browser": "^8.1.1",
    "@angular/platform-browser-dynamic": "^8.1.1",
    "@angular/router": "^8.1.1",
    "@swimlane/ngx-charts": "^12.0.1",
    "angular-oauth2-oidc": "^8.0.4",
    "chroma-js": "~2.0.2",
    "date-fns": "^2.4.1",
    "google-protobuf": "^3.8.0",
    "grpc-web": "^1.0.5",
    "hammerjs": "^2.0.8",
    "iso-639-3": "^1.1.0",
    "nouislider": "^12.1.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@types/chroma-js": "^1.4.1",
    "@types/google-protobuf": "^3.7.1",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.18",
    "@types/nouislider": "^9.0.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ngx-i18nsupport": "^0.17.0",
    "protractor": "~5.4.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.1",
    "typescript": "~3.4.5"
  }
